{
  "timeout": 500,
  "sheets": [
    {
      "name": "users",
      "importerClass": "App\\Imports\\Importers\\UserImporter",
      "validatorClass": "App\\Imports\\Validators\\CustomValidator",
      "chunkSize": 250,
      "columns": [
        {
          "name": "title",
          "validations": "string|in:Miss,Ms,Mr"
        },
        {
          "name": "first_name",
          "validations": "string|required"
        },
        {
          "name": "last_name",
          "validations": "string|required"
        },
        {
          "name": "other_name",
          "validations": "string"
        },
        {
          "name": "email",
          "validations": "email|required"
        },
        {
          "name": "password",
          "validations": "required|min:6"
        },
        {
          "name": "role",
          "validations": "string|required|in:admin,teacher, schooladmin"
        },
        {
          "name": "gender",
          "validations": "string|required|in:male,female"
        }
      ]
    }
  ]
}